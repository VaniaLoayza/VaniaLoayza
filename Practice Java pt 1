import java.util.Scanner;
class Main{
  public static void main(String[] args) {
    Scanner input=new Scanner(System.in);
    String str=input.nextline();
    int count=0;
    for(int i = 0; i < str.length(); i++) {
      if(str.charAt(i) != '')
      count++;
    }
  }
}

